{
  "assignMethod": "APPEND",
  "gitLogMessages": {
    "feat": ["/^feat/i"],
    "fix": ["/^fix/i"],
    "perf": ["/^perf/i"],
    "build": ["/^build/i"],
    "ci": ["/^ci/i"],
    "revert": ["/^revert/i"],
    "docs": ["/^docs/i"],
    "style": ["/^style/i"],
    "refactor": ["/^refactor/i"],
    "test": ["/^test/i"],
    "chore": ["/^chore/i"],
    "patch": ["/^(feat|fix|perf|build|ci|revert|docs|style|refactor|test|chore)/i"],
    "minor": ["/^feat/i"],
    "major": ["/^(\\w+(\\(\\w*\\))?!|BREAKING CHANGE)/i"]
  },
  "gitDiffPaths": {}
}
