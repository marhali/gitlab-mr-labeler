{
  "assignMethod": "APPEND",
  "gitLogMessages": {
    "feat": ["/^feat/mi"],
    "fix": ["/^fix/mi"],
    "perf": ["/^perf/mi"],
    "build": ["/^build/mi"],
    "ci": ["/^ci/mi"],
    "revert": ["/^revert/mi"],
    "docs": ["/^docs/mi"],
    "style": ["/^style/mi"],
    "refactor": ["/^refactor/mi"],
    "test": ["/^test/mi"],
    "chore": ["/^chore/mi"],
    "patch": ["/^(feat|fix|perf|build|ci|revert|docs|style|refactor|test|chore)/mi"],
    "minor": ["/^feat/mi"],
    "major": ["/^(\\w+(\\(\\w*\\))?!|BREAKING CHANGE)/mi"]
  },
  "gitDiffPaths": {}
}
